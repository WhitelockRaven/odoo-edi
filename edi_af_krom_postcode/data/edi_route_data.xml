<openerp>
    <data noupdate="1">
        <record id="asok_postcode_route" model="edi.route"> 
            <field name="name">AF Asok: Get KROM postcode route</field> 
            <field name="partner_id" ref="base.main_partner"/> 
            <field name="active" eval="True"/> 
            <field name="protocol">ipf</field> 
            <field name="route_type">edi_af_as_krom_postcode</field> 
        </record>

        <record id="asok_postcode" model="edi.message.type">
            <field name="name">AF asok: Get KROM postcode</field>
            <field name="target">ais-bos-regelverk/api/Krom/ArPostnummerGiltigForKrom</field>
            <field name="type_mapping">{{url}}:{{port}}/{path}?client_id={{client}}&amp;client_secret={{secret}}&amp;postnummer={postnummer}</field>
        </record>

    </data>
</openerp>